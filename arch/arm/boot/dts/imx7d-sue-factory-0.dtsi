			MX7D_PAD_EPDC_DATA03__GPIO2_IO3			0x10
			MX7D_PAD_LCD_DATA06__GPIO3_IO11			0x10

			MX7D_PAD_LCD_DATA07__GPIO3_IO12			0x10
			MX7D_PAD_LCD_DATA05__GPIO3_IO10			0x10

			MX7D_PAD_LCD_DATA08__GPIO3_IO13			0x10
			MX7D_PAD_LCD_DATA13__GPIO3_IO18			0x10

			MX7D_PAD_LCD_DATA09__GPIO3_IO14			0x10
