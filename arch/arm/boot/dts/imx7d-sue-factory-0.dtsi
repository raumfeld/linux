&iomuxc_lpsr {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_factory_test_lpsr_gpios>;
	imx7d-sue-demo {
		pinctrl_factory_test_lpsr_gpios: factory_test_lpsr_gpios {
			fsl,pins = <
			MX7D_PAD_GPIO1_IO05__GPIO1_IO5 0x10
			MX7D_PAD_GPIO1_IO03__GPIO1_IO3 0x10
			MX7D_PAD_GPIO1_IO01__GPIO1_IO1 0x10
			>;
		};
	};
};

&mdio_bus {
	ethphy1: ethernet-phy@0 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <0>;
	};
};
