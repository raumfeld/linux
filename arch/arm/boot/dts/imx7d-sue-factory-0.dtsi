			MX7D_PAD_LCD_DATA10__GPIO3_IO15			0x10
			MX7D_PAD_LCD_DATA12__GPIO3_IO17			0x10

			MX7D_PAD_LCD_DATA15__GPIO3_IO20			0x10

			MX7D_PAD_I2C1_SCL__GPIO4_IO8			0x10
			MX7D_PAD_I2C2_SDA__GPIO4_IO11			0x10

			MX7D_PAD_I2C2_SCL__GPIO4_IO10			0x10
			MX7D_PAD_LCD_DATA16__GPIO3_IO21			0x10

			MX7D_PAD_LCD_VSYNC__GPIO3_IO3			0x10
			MX7D_PAD_EPDC_BDR1__GPIO2_IO29			0x10

			MX7D_PAD_SAI2_RX_DATA__GPIO6_IO21		0x10
			MX7D_PAD_UART2_RX_DATA__GPIO4_IO2		0x10

			MX7D_PAD_SD1_CMD__GPIO5_IO4			0x10
