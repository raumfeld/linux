			MX7D_PAD_SD1_DATA1__GPIO5_IO6			0x10
			MX7D_PAD_LCD_DATA02__GPIO3_IO7			0x10
			MX7D_PAD_ENET1_RX_CLK__GPIO7_IO13		0x10
			MX7D_PAD_SAI1_RX_DATA__GPIO6_IO12		0x10

