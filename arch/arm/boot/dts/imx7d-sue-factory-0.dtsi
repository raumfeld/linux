			MX7D_PAD_SD1_CLK__GPIO5_IO3			0x10
			MX7D_PAD_UART3_RX_DATA__GPIO4_IO4		0x10

			MX7D_PAD_SAI2_TX_DATA__GPIO6_IO22		0x10
			MX7D_PAD_GPIO1_IO12__GPIO1_IO12			0x10
			MX7D_PAD_SAI2_TX_SYNC__GPIO6_IO19		0x10

			MX7D_PAD_UART2_TX_DATA__GPIO4_IO3		0x10
			MX7D_PAD_SAI2_TX_BCLK__GPIO6_IO20		0x10

			MX7D_PAD_GPIO1_IO10__GPIO1_IO10			0x10
